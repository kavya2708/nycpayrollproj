{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredfnycpayroll"
		},
		"ls_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql'"
		},
		"ls_storageacc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_storageacc'"
		},
		"ls_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nycpayserver.database.windows.net"
		},
		"ls_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_nycpayroll"
		},
		"ls_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ls_storageacc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nycpayroll.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredfnycpayroll_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_password": {
						"value": "[parameters('ls_sql_password')]"
					},
					"ls_storageacc_accountKey": {
						"value": "[parameters('ls_storageacc_accountKey')]"
					},
					"ls_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_properties_typeProperties_server')]"
					},
					"ls_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_properties_typeProperties_database')]"
					},
					"ls_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_properties_typeProperties_userName')]"
					},
					"ls_storageacc_properties_typeProperties_url": {
						"value": "[parameters('ls_storageacc_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azuredfnycpayroll_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azuredfnycpayroll_ArmTemplate_0')]"
			]
		}
	]
}